{
  "manifest_version": 3,
  "name": "CookieWizard",
  "description": "Cookie Wizard Chrome extension",
  "version": "0.1.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "CookieWizard"
  },
  "permissions": [
    "cookies",
    "activeTab",
    "favicon",
    "contextMenus",
    "tabs",
    "notifications",
    "webRequest",
    "storage"
  ],
  "host_permissions": ["http://*/*", "https://*/*", "<all_urls>"],
  "web_accessible_resources": [
    {
      "resources": ["_favicon/*"],
      "matches": ["<all_urls>"],
      "extension_ids": ["*"]
    }
  ]
}
